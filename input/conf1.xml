<CARMINE>
	<SYSTEM>
		<MACHINE id="1" cpu="5.5" memory="32" allocate="50"/>
		<MACHINE id="2" cpu="3.5" memory="16" allocate="30"/>
		<MACHINE id="3" cpu="2" memory="8" allocate="10"/>
		<CONNECTION between="1<->2" speed="10"/>
		<CONNECTION between="2<->3" speed="52"/>
		<CONNECTION between="1<->3" speed="5"/>
	</SYSTEM>

	<ASSERTION avoid_fn="1" critical_condition="threshold1 > 30"> G(a -> X(b)) </ASSERTION>
	<ASSERTION avoid_fp="1" critical_condition="d > 90"> G(d > 50 -> e U f) </ASSERTION>

	<TOPIC name="topic1" frequency="50"/>
	<TOPIC name="topic2" frequency="10"/>


</CARMINE>
